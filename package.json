{
  "name": "auth-service",
  "version": "1.0.0",
  "description": "",
  "main": "app.ts",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.421.0",
    "@aws-sdk/client-secrets-manager": "^3.431.0",
    "@aws-sdk/s3-request-presigner": "^3.421.0",
    "@azure/msal-node": "^2.5.1",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.1",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cross-var": "^1.1.0",
    "env-cmd": "^10.1.0",
    "express": "^4.18.2",
    "express-fileupload": "^1.4.1",
    "express-jwt": "^8.4.1",
    "fs": "^0.0.1-security",
    "google-auth-library": "^9.2.0",
    "helmet": "^7.0.0",
    "http": "^0.0.1-security",
    "joi": "^17.10.2",
    "jsonwebtoken": "^9.0.2",
    "mime": "^3.0.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "shx": "^0.3.4",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "twilio": "^4.19.0",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "typescript": "^5.2.2",
    "useragent": "^2.3.0",
    "uuid": "^9.0.1"
  },
  "scripts": {
    "tsc": "tsc",
    "env": "env-cmd -f .env --silent",
    "start": "npm run env -- node src/app.js",
    "start:dev": "npm run env -- ts-node src/app.ts",
    "dev": "npm run env -- nodemon src/app.ts",
    "typeorm": "npm run env -- typeorm-ts-node-esm -d ./src/orm/ormconfig.ts",
    "migration:generate": "cross-var npm run typeorm -- migration:generate ./src/orm/migrations/$npm_config_name",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "migration:revert:fake": "npm run typeorm -- migration:revert -f",
    "migration:show": "npm run typeorm -- migration:show",
    "build": "shx rm -rf build && npx tsc && shx cp ./package* ./build/ && shx cp .env* ./build/",
    "seed": "env-cmd -f .env ts-node ./src/orm/seed/index.ts"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.3",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/express-fileupload": "^1.4.2",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.0",
    "@types/passport": "^1.0.13",
    "@types/uuid": "^9.0.4",
    "typescript": "^5.2.2"
  }
}
